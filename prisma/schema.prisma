generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model BlogPost {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  title     String
  desc      String
  author    String
  content   String
  tags      String
  image     String
  createdAt DateTime @default(now())
}
